{
  "name": "facaade",
  "version": "0.2.12",
  "description": "A style agnostic react component library",
  "main": "./dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run test:watch",
    "build": "babel --source-maps --ignore=src/testSetup,**/*.test.js src/ -d dist/",
    "test": "jest --coverage",
    "test:watch": "jest --watch --verbose",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "author": "Aaron Moore <hi@madebyaaron.xyz>",
  "license": "ISC",
  "sideEffects": false,
  "devDependencies": {
    "@babel/preset-stage-0": "^7.0.0",
    "@storybook/react": "^3.4.10",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-sass-export": "^0.0.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.6.0",
    "node-sass": "^4.9.4",
    "prop-types": "^15.6.2",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "sinon": "^6.3.1",
    "style-loader": "^0.23.1"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/src/setup"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>src/testSetup/setupEnzyme.js",
    "moduleNameMapper": {
      "^.*\\.scss$": "<rootDir>src/testSetup/scssStub.js"
    }
  },
  "dependencies": {
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "styled-components": "^4.0.2"
  }
}
